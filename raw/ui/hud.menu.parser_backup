	#define	SCORE_INDEX		"0"
	
	itemDef	{ name	"player0_bg"
		rect			(SCORE_BG_OFFSET_X - 35) (SCORE_HUD_Y_0 + SCORE_BG_OFFSET_Y) 110 30
		forecolor		SCORE_BG_COLOR
		style			WINDOW_STYLE_SHADER
		background		"hud_bg_blood"
		decoration
		visible			1
	}
	itemDef { name	"player0_score" 
		type			ITEM_TYPE_TEXT
		rect			SCORE_HUD_OFFSET_X SCORE_HUD_Y_0 100 0
		textscale		SCORE_HUD_COOP_TEXTSCALE
		textstyle		ITEM_TEXTSTYLE_SHADOWED
		textfont		UI_FONT_BOLD
		forecolor		WHITE_GRADIENT HUD_FOREGROUND_ALPHA
		exp text		PLAYER_0_SCORE
		textalign 		ITEM_ALIGN_RIGHT
		visible 		when(SCORE_INDEX != PLAYER_INDEX)
		decoration
	}
	itemDef { name "player0_score_mini" //used when teammates have two separate scores (ex. Chaos Mode) 
		type			ITEM_TYPE_TEXT
		rect			SCORE_HUD_OFFSET_X (SCORE_HUD_Y_0 + SCORE_HUD_Y_MINIOFFSET) 100 0
		textscale		SCORE_HUD_MINI_TEXTSCALE
		textstyle		ITEM_TEXTSTYLE_SHADOWED
		textfont		UI_FONT_NORMAL
		forecolor		WHITE_GRADIENT 1
		exp text		PLAYER_0_MINISCORE
		textalign 		ITEM_ALIGN_RIGHT
		visible 		when(PLAYER_0_MINISCORE != "no")
		decoration
	}
	itemDef { name "player0_score_large" 
		type			ITEM_TYPE_TEXT
		rect			SCORE_HUD_OFFSET_X (SCORE_HUD_Y_0 + SCORE_HUD_Y_L) 100 0
		textscale		SCORE_HUD_TEXTSCALE
		textstyle		ITEM_TEXTSTYLE_SHADOWED
		textfont		UI_FONT_BOLD
		forecolor		WHITE_GRADIENT HUD_FOREGROUND_ALPHA
		exp text		PLAYER_0_SCORE
		textalign 		ITEM_ALIGN_RIGHT
		visible 		when(SCORE_INDEX == PLAYER_INDEX)
		decoration
	}
	itemDef { name "player0_medal" 
		style			WINDOW_STYLE_SHADER 
		rect			(SCORE_BG_OFFSET_X + SCORE_METAL_OFFSET_X - 35) (SCORE_HUD_Y_0 + SCORE_METAL_OFFSET_Y + SCORE_BG_OFFSET_Y) 20 20
